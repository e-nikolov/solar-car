%%%% 1 %%%%
	[true*.pressOff] mu X. [!conMotorToBattery(0)] X			%true
	&& [true*.pressOff] mu X. [!conBatteryToMotor(0)] X			%true
	&& [true*.pressOff] mu X. [!conSolarPanelToMotor(0)] X		%true
	&& [true*.pressOff] mu X. [!conSolarPanelToBattery(0)] X	%true

%	&& forall  m: Nat . val(m>0&&m<=5)=>   ([true*.(conMotorToBattery(m)+conBatteryToMotor(m)+conSolarPanelToMotor(m)+conSolarPanelToBattery(m)+conSolarPanelToBattery(m)).(!pressOn)*. pressOff]false)		%false


%%%% 2 %%%%
	&& [true*.enterSafeMode] mu X. [!pressOff] X			  %true

%%%% 3 %%%%
	&& [true*.pressBrake] mu X. [!conBatteryToMotor(0)] X		%true
	&& [true*.pressBrake] mu X. [!conSolarPanelToMotor(0)] X	%true
%	&& forall m: Nat . val(m>0&&m<=5)=> ([true*.(conBatteryToMotor(m)+conSolarPanelToMotor(m)). (!pressBrake)*. releaseBrake]false)	%false

%%%% 4 %%%%

%	&& [true*(getBatteryLevel(Empty) || getBatteryLevel(Medium) && getBatteryTemp(Normal))!(getBatteryLevel(Full)&&getBatteryTemp(Overheating))*. pressBrake] mu X . [!(forall p: Nat. val(p>=0&&p<=5)=> (conMotorToBattery(p)))] X		% syntax error

%%%% 5 %%%%
	&& [true*. (getBatteryTemp(Overheating)||getBatteryLevel(Full))] mu X. [!(conSolarPanelToBattery(0)) && !(conMotorToBattery(0))] X	%true
%	&& forall m: Nat . val(m>0&&m<=5)=> ([true*. (conMotorToBattery(m)+conSolarPanelToBattery(m)). !(getBatteryLevel(Medium)||getBatteryLevel(Empty)&&getBatteryTemp(Normal))*. (getBatteryLevel(Full)||getBatteryTemp(Overheating))]false)	% false
